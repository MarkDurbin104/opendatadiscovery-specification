{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "definitions": {
        "S3DataSetExtension": {
            "type": "object",
            "properties": {
                "files": {
                    "type": "int"
                },
                "bucket": {
                    "type": "string"
                },
                "key": {
                    "type": "string"
                },
                "region": {
                    "type": "string"
                },
                "avgFileSize": {
                    "type": "int"
                },
                "estimatedDatasetSize": {
                    "type": "string"
                },
                "fileFormat": {
                    "type": "string",
                    "enum": ["csv", "tsv", "parquet"]
                },
                "partitionStyle": {
                    "type": "string",
                    "enum": ["Directory partitioned", "Hive style partitioned"]
                },
                "partitioningColumns": {
                    "type": "array"
                }
            }
        
        }
    }
}
